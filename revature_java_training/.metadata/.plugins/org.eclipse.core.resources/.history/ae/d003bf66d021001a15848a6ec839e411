package example1;

public class CharApi {

	public static void main(String[] args) {
	String s="H8elLLOO 123@##)_Hi  K";
	int alpha=0;
	
	for (int i = 0; i < s.length(); i++) {
		char c=s.charAt(i);
		if(Character.isAlphabetic(c)) {
			alpha++;
		}
	}
	System.out.println("Alpha count "+alpha);
	
	}

}
