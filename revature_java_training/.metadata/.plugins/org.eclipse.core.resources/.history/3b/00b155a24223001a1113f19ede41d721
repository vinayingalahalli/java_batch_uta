package by_thread_class;

public class MyClass {

	public static void main(String[] args) {
		
		System.out.println(Thread.currentThread());
		Thread t=Thread.currentThread();
		t.setPriority(8);
		t.setName("my_main");
		System.out.println(Thread.currentThread());
		System.out.println(8/0);
		
		MyThread t1=new MyThread();
		t1.setName("t1");
	

	}

}
